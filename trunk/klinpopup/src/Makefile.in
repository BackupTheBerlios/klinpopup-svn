klinpopupview_base.h: $(srcdir)/klinpopupview_base.ui
	rm -rf klinpopupview_base.h;
	$(UIC) $(srcdir)/klinpopupview_base.ui | $(PERL) -pi -e "s,public QWizard,public KWizard,g; s,#include <qwizard.h>,#include <kwizard.h>,g" >> klinpopupview_base.h ;
klinpopupview_base.moc: klinpopupview_base.h
	$(MOC) klinpopupview_base.h -o klinpopupview_base.moc

settings.cpp: settings.h
settings.h: $(srcdir)/klinpopup.kcfg $(srcdir)/settings.kcfgc $(KCFG_DEPENDENCIES)
	$(KCONFIG_COMPILER) $(srcdir)/klinpopup.kcfg $(srcdir)/settings.kcfgc; ret=$$?; \
	if test "$$ret" != 0; then rm -f settings.h ; exit $$ret ;  fi

